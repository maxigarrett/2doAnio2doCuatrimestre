package POOTarea;
import java.util.ArrayList;

import POOTarea.alumno;
public class case2 
{
	ArrayList<alumno> mialumno=new ArrayList<alumno>();
	public void borrarTodo() 
	{
		for(int i=0;i<mialumno.size();i++) 
		{
			mialumno.clear();
		}
		
		System.out.println("se borro correctamente");
		
	}
}
